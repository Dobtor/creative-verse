<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <template id="assets_frontend" inherit_id="website.assets_frontend" name="Dobtor Demand Frontend Assets">
            <xpath expr="//script[last()]" position="after">
                <script type="text/javascript" src="/dobtor_demand_marketplace/static/src/js/team_demand_option.js"/>
                <script type="text/javascript" src="/dobtor_demand_marketplace/static/src/js/demand_portal_upload.js"/>
                <script type="text/javascript" src="/dobtor_demand_marketplace/static/src/js/demand_portal_edit.js"/>
            </xpath>
            <xpath expr="//link[last()]" position="after">
                <link rel="stylesheet" type="text/scss" href="/dobtor_demand_marketplace/static/src/scss/demand_record.scss"/>
                <link rel="stylesheet" type="text/scss" href="/dobtor_demand_marketplace/static/src/scss/service_record.scss"/>
            </xpath>
        </template>

    </data>
</odoo>
